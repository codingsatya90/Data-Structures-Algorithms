package dsa.function.in.sit;

public class EligibleForVote {
	
	public static void vote(int age) {
		
		if(age >=18) {
			System.out.println("Eligible for Vote");
		}else {
			System.out.println("Not  Eligible for Vote");
		}
		
	}
	
	public static void main(String[] args) {
		
		int age=18;
		vote(age);
		
	}
	
	

}
