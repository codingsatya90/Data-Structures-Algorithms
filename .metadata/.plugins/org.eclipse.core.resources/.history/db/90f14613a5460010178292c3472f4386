package dsa.array.in.sit;

import java.util.Scanner;

public class FindMaxMinValues {
	public static void main(String[] args) {
		
		int arr[]= {2,5,6,1,7,3};
		int min=0;
		int max=0;
		for(int i=0; i<arr.length;i++) {
			if(arr[i]>max) {
				max=arr[i];
			}else {
				min=arr[i];
			}
		}
		System.out.println(max);
		System.out.println(min);
		
	}

}
